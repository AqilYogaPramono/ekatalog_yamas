<%- include('../../../../partial/__messages.ejs') %>
<%- include('../../../../partial/__head.ejs', { title: 'Daftar Lantai' }) %>
<%- include('../../../../partial/__nav-side.ejs') %>

<div class="col-12 grid-margin stretch-card">
    <div class="card">
      <div class="card-body">
        <h4 class="card-title">Edit lantai</h4>
        <form class="forms-sample" action="/pengurus/lantai/update/<%= data.id %>" method="POST">
          <div class="form-group">
            <label for="inputlantai">lantai</label>
            <input type="text" class="form-control" id="inputlantai" name="kode_lantai" placeholder="Inputkan lantai" value="<%= data.kode_lantai %>" required>
          </div>
          <button type="submit" class="btn btn-primary mr-2">Update</button>
          <a href="/pengurus/lantai" class="btn btn-light">Cancel</a>
        </form>
      </div>
    </div>
  </div>

<%- include('../../../../partial/__script.ejs') %>