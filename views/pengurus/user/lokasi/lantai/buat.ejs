<%- include('../../../../partials1/header/__header.ejs', { title: 'Tambah Lantai' }) %>
<%- include('../../../../partials1/nav-side/__nav-sidePengurus.ejs') %>
<%- include('../../../../partials1/messages/messagesDashboard.ejs') %>

<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">
            <div class="col-12 grid-margin stretch-card">
              <div class="card">
                <div class="card-body">
                  <h4 class="card-title">Tambah Lantai</h4>
                  <form action="/pengurus/lantai/create" method="POST" data-loading>
                    <div class="form-group">
                      <label for="kode_lantai">Kode Lantai</label>
                      <input type="text" class="form-control" id="kode_lantai" name="kode_lantai" required>
                    </div>
                    <div class="d-flex">
                      <button type="submit" class="btn btn-primary mr-2">Simpan</button>
                      <a href="/pengurus/lantai" class="btn btn-light" data-loading>Kembali</a>
                    </div>
                  </form>
                </div>
              </div>
            </div>
        </div>
    </div>
</div>

<%- include('../../../../partials1/script/__script.ejs') %>