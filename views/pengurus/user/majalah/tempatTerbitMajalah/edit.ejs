<%- include('../../../../partial/__messages.ejs') %>
<%- include('../../../../partial/__head.ejs', { title: 'Daftar Penerbit Majalah' }) %>
<%- include('../../../../partial/__nav-side.ejs') %>

<div class="col-12 grid-margin stretch-card">
    <div class="card">
      <div class="card-body">
        <h4 class="card-title">Edit penerbit-majalah</h4>
        <form class="forms-sample" action="/pengurus/penerbit-majalah/update/<%= data.id %>" method="POST">
          <div class="form-group">
            <label for="inputPenerbitMajalah">Penerbit Majalah</label>
            <input type="text" class="form-control" id="inputPenerbitMajalah" name="nama_tempat_terbit_majalah" placeholder="Inputkan Penerbit Majalah" value="<%= data.nama_tempat_terbit_majalah %>" required>
          </div>
          <button type="submit" class="btn btn-primary mr-2">Update</button>
          <a href="/pengurus/penerbit-majalah" class="btn btn-light">Cancel</a>
        </form>
      </div>
    </div>
  </div>

<%- include('../../../../partial/__script.ejs') %>